[
    {
        "$project": {
            "data.1": {
                "$toInt": "$data.1"
            }
        }
    },
    {
        "$group": {
            "_id": "blueberry.option",
            "count": {
                "$sum": 1
            },
            "1": {
                "$sum": "$data.1"
            }
        }
    },
    {
        "$merge": {
            "into": "resultss",
            "on": "_id",
            "whenMatched": "replace",
            "whenNotMatched": "insert"
        }
    }
]
