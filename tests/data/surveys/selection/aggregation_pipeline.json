[
    {
        "$project": {
            "data.1.1": {
                "$toInt": "$data.1.1"
            },
            "data.1.2": {
                "$toInt": "$data.1.2"
            },
            "data.1.3": {
                "$toInt": "$data.1.3"
            }
        }
    },
    {
        "$group": {
            "_id": "blueberry.selection",
            "count": {
                "$sum": 1
            },
            "1+1": {
                "$sum": "$data.1.1"
            },
            "1+2": {
                "$sum": "$data.1.2"
            },
            "1+3": {
                "$sum": "$data.1.3"
            }
        }
    },
    {
        "$merge": {
            "into": "resultss",
            "on": "_id",
            "whenMatched": "replace",
            "whenNotMatched": "insert"
        }
    }
]
